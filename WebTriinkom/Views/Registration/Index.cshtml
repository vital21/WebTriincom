@model IEnumerable<WebTriinkom.Models.User>

<h2>Список пользователей</h2>
<p><a asp-action="Create">Добавить пользователя</a></p>
<table class="table">
    <tr><th>Имя</th><th>Возраст</th><th>Пароль</th></tr>
    @foreach (var item in Model)
    {
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Password</td>
            </tr>
    }
</table>